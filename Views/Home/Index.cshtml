@model List<chefsndishes.Models.Chef>
<div class="container">
    <div class="row" style="margin-top: 15px">
        <div class="col-sm-10" style="text-align: center">
            <h1>Chefs  |  <a href="/dishes">Dishes</a></h1>
        </div>
        <div class="col-sm-2">
            <a href="/new">Add a Chef</a>
        </div>
    </div>
    <div class="row" style="margin: 25px">
        <h2>Check out our roster of Chefs!</h2>
        <hr>
    </div>
    <table class="table table-striped">
        <thead>
            <th>Name</th>
            <th>Age</th>
            <th># of Dishes</th>
        </thead>
        <tbody>
            @foreach(Chef item in Model)
            {
            <tr>
                <td>@item.FirstName @item.LastName</td>
                <td>@item.Age</td>
                <td>@item.CreatedDishes.Count</td>
            </tr>
            }
        </tbody>
    </table>
</div>